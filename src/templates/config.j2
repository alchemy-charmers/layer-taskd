confirmation=1
extensions=/usr/libexec/taskd
ip.log=on
log=/tmp/taskd.log
pid.file=/run/taskd.pid
queue.size=10
request.limit=1048576
root=/var/lib/taskd
server={{ server }}
trust=strict
verbose=1
client.cert=/usr/share/taskd/pki/client.cert.pem
client.key=/usr/share/taskd/pki/client.key.pem
server.cert=/usr/share/taskd/pki/server.cert.pem
server.key=/usr/share/taskd/pki/server.key.pem
server.crl=/usr/share/taskd/pki/server.crl.pem
ca.cert=/usr/share/taskd/pki/ca.cert.pem
